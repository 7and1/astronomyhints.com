{
  "name": "orbit-command",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @opennextjs/cloudflare",
    "pages:deploy": "npm run build && npm run pages:build && wrangler pages deploy .open-next/worker",
    "pages:dev": "wrangler pages dev .open-next/worker --compatibility-date=2024-09-23 --compatibility-flag=nodejs_compat",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:a11y": "vitest run --grep accessibility",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "typecheck": "tsc --noEmit",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:preview": "./scripts/deploy.sh preview",
    "validate-env": "./scripts/validate-env.sh",
    "smoke-test": "./scripts/smoke-tests.sh",
    "backup": "./scripts/backup.sh",
    "backup:list": "./scripts/backup.sh list",
    "backup:restore": "./scripts/backup.sh restore",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@react-three/drei": "9.122.0",
    "@react-three/fiber": "8.18.0",
    "@react-three/postprocessing": "2.19.1",
    "astronomy-engine": "2.1.19",
    "gsap": "3.14.2",
    "lucide-react": "0.468.0",
    "next": "^15.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "0.170.0",
    "zustand": "5.0.11"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.0",
    "@opennextjs/cloudflare": "1.16.2",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/three": "^0.170.0",
    "@vitest/coverage-v8": "^2.1.0",
    "@vitest/ui": "^2.1.0",
    "autoprefixer": "^10.4.20",
    "axe-core": "^4.8.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.6",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vitest": "^2.1.0",
    "wrangler": "4.62.0"
  }
}